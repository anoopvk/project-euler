    "import time\n",
    "start=time.time()\n",
    "import math\n",
    "\n",
    "a=[0]*2000001\n",
    "s=0\n",
    "for i in range(2,2000000):\n",
    "    if a[i]== 0:\n",
    "        s=s+i\n",
    "        if(i<math.sqrt(2000000)):\n",
    "            for j in range(2,(2000000//i)+1):\n",
    "                a[i*j]=1\n",
    "print(s)\n",
    "\n",
    "\n",
    "end=time.time()\n",
    "print(end-start)"
